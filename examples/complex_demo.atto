(* Complex Numbers Demo *)
open Complex

(* Test 1: Add two complex numbers *)
add (C (1.0, 2.0)) (C (3.0, 4.0))

(* Test 2: Multiply complex numbers *)
mul (C (2.0, 3.0)) (C (4.0, 5.0))

(* Test 3: Conjugate and magnitude squared *)
let c = C (3.0, 0.0 - 4.0) in
magnitudeSquared (conj c)

(* Test 4: Polar conversion *)
let c2 = C (1.0, 1.0) in
toPolar c2
